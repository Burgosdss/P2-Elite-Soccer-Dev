<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Team Name</th>
      <th>Location</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% teams.forEach(function(t) { %>
      <tr>
        <td><%= t.teamname %></td>
        <td><%= t.location %></td>
        <td><a href="/teams/<%= t._id %>">More Details</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>